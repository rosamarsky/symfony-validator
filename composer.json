{
    "name": "rosamarsky/symfony-validator",
    "version": "1.0",
    "description": "Simple annotation validator for Symfony framework",
    "keywords": [
        "symfony",
        "validator",
        "annotation",
        "clear controllers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Samarsky",
            "email": "rosamarsky@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "*",
        "symfony/config": "~2.7 || ~3.3 || ~4.0",
        "symfony/dependency-injection": "~2.7 || ~3.3 || ~4.0",
        "symfony/http-kernel": "~2.7 || ~3.0 || ~4.0",
        "symfony/framework-bundle": "~2.7 || ~3.0 || ~4.0",
	    "symfony/yaml": "~2.7 || ~3.3 || ~4.0"
    },
    "require-dev": {
	    "phpunit/phpunit": "^5.7 || ^6.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Rosamarsky\\Validator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rosamarsky\\Validator\\Tests\\": "tests"
        }
    }
}
